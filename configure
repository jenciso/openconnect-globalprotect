{ $as_echo "$as_me:$LINENO: checking for CWRAP" >&5
$as_echo_n "checking for CWRAP... " >&6; }

if test -n "$CWRAP_CFLAGS"; then
    pkg_cv_CWRAP_CFLAGS="$CWRAP_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { ($as_echo "$as_me:$LINENO: \$PKG_CONFIG --exists --print-errors \"uid_wrapper, socket_wrapper\"") >&5
  ($PKG_CONFIG --exists --print-errors "uid_wrapper, socket_wrapper") 2>&5
  ac_status=$?
  $as_echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }; then
  pkg_cv_CWRAP_CFLAGS=`$PKG_CONFIG --cflags "uid_wrapper, socket_wrapper" 2>/dev/null`
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$CWRAP_LIBS"; then
    pkg_cv_CWRAP_LIBS="$CWRAP_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { ($as_echo "$as_me:$LINENO: \$PKG_CONFIG --exists --print-errors \"uid_wrapper, socket_wrapper\"") >&5
  ($PKG_CONFIG --exists --print-errors "uid_wrapper, socket_wrapper") 2>&5
  ac_status=$?
  $as_echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }; then
  pkg_cv_CWRAP_LIBS=`$PKG_CONFIG --libs "uid_wrapper, socket_wrapper" 2>/dev/null`
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        CWRAP_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors "uid_wrapper, socket_wrapper" 2>&1`
        else
	        CWRAP_PKG_ERRORS=`$PKG_CONFIG --print-errors "uid_wrapper, socket_wrapper" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$CWRAP_PKG_ERRORS" >&5

	{ $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
                have_cwrap=no
elif test $pkg_failed = untried; then
	have_cwrap=no
else
	CWRAP_CFLAGS=$pkg_cv_CWRAP_CFLAGS
	CWRAP_LIBS=$pkg_cv_CWRAP_LIBS
        { $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
	have_cwrap=yes
fi
